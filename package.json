{
    "name": "prueba-sequelize-cli",
    "version": "1.0.0",
    "description": "",
    "main": "server.js",
    "scripts": {
        "start-dev": "nodemon server.js",
        "show-users": "sqlite3 database.sqlite3 -cmd 'SELECT * FROM user;'",
        "seed": "npx sequelize-cli db:seed:all",
        "migrate": "npx sequelize-cli db:migrate",
        "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate && npx sequelize-cli db:seed:all  ",
        "test": "cross-env NODE_ENV=test jest  --forceExit --json",
        "pretest": "cross-env NODE_ENV=test npm run migrate:reset"
    },
    "jest": {
        "testEnvironment": "node",
        "coveragePathIgnorePatterns": [
            "/node_modules/"
        ]
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "nodemon": "^2.0.6",
        "sequelize": "^6.3.5",
        "sequelize-cli": "^6.2.0",
        "sqlite3": "^5.0.0"
    },
    "devDependencies": {
        "cross-env": "^7.0.2",
        "jest": "^26.6.3",
        "supertest": "^6.0.1"
    }
}
